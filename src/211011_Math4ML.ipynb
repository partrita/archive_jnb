   "source": [
    "%matplotlib inline\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "def n_func(x):\n",
    "    return 3*x**2+2*x+1\n",
    "\n",
    "x = np.linspace(-2,2)\n",
    "y = n_func(x)\n",
    "\n",
    "plt.plot(x,y)"
   ]
